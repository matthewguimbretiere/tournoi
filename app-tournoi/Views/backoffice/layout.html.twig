  <link rel="stylesheet" href="{{ asset( '/css/backoffice.css' ) }}">
</head>
<body>
	 <div class="wrapper">
		 <!-- Sidebar  -->
        	{% include '/_partials/_sidebar.html.twig' %}
        <!-- Page Content  -->
        <div id="content">

            {% include '/_partials/_navbar.html.twig' %}

            <div class="content-wrapper">
                {% block content_back %}{% endblock %}
            </div>
        </div>
	</div>
</body>

